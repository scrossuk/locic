// Test output.
import void testPrint(const ubyte * const str, ...);

// Entry point.
export int testEntryPoint(unused int argc, unused ubyte ** argv) {
	int v = 1;
	
	testPrint(C"assert v == 1");
	assert v == 1;
	
	v += 1;
	
	testPrint(C"assert v == 2");
	assert v == 2;
	
	v *= 2;
	
	testPrint(C"assert v == 4");
	assert v == 4;
	
	v -= 5;
	
	testPrint(C"assert v == -1");
	assert v == -1;
	
	return 0;
}

